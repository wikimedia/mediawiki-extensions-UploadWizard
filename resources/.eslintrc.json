{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"wikibase": "readonly"
	},
	"rules": {
		"max-len": "off",
		"jsdoc/no-undefined-types": [
			"warn",
			{
				"definedTypes": [
					"uw",
					"uw.controller.Step",
					"uw.DetailsWidget",
					"uw.FieldLayout",
					"uw.PatentDialog",
					"uw.UlsWidget",
					"uw.ui.Step",
					"uw.ValidatableElement",
					"uw.ValidationStatus"
				]
			}
		],
		"no-shadow": "warn",
		"no-jquery/no-done-fail": "warn"
	},
	"settings": {
		"jsdoc": {
			"preferredTypes": {
				"StatementWidget": "StatementWidget",
				"wikibaseDatamodel": "wikibaseDatamodel"
			}
		}
	}
}
